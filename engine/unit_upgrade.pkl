module unit_upgrade
extends "child.pkl"
import "types.pkl" as $t

fixed type = "upgrade"
fixed subtype = "upgrade"

tier: $t.UpgradeTier
flux_cost: Number = 
  if (tier == "T1.5") 100 else
  if (tier == "T2.5") 150 else 
  if (tier == "T3.5") 200 else 
  throw("do not know flux cost for tier \(tier)")
